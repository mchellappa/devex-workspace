{
  "name": "devex-ai-assistant",
  "displayName": "DevEx AI Assistant",
  "description": "Complete SDLC acceleration platform: LLD review, OpenAPI generation, Spring Boot code generation, deployment automation, and productivity analytics. AI-powered by GitHub Copilot.",
  "version": "1.2.7",
  "publisher": "CodeSamurai",
  "icon": "images/icon.png",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mchellappa/devex-workspace.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Linters",
    "Testing",
    "Other"
  ],
  "keywords": [
    "spring boot",
    "java",
    "openapi",
    "code generation",
    "ai",
    "copilot",
    "productivity",
    "lld review",
    "api design",
    "architecture review",
    "devops",
    "sdlc",
    "automation"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "devex.generateSpringBootProject",
        "title": "Generate Spring Boot Project from Design",
        "category": "DevEx"
      },
      {
        "command": "devex.summarizeLLD",
        "title": "Summarize LLD Document",
        "category": "DevEx"
      },
      {
        "command": "devex.reviewLLD",
        "title": "Review LLD",
        "category": "DevEx"
      },
      {
        "command": "devex.parseOpenAPI",
        "title": "Parse OpenAPI Specification",
        "category": "DevEx"
      },
      {
        "command": "devex.generateOpenAPISpec",
        "title": "Generate OpenAPI Spec from LLD",
        "category": "DevEx"
      },
      {
        "command": "devex.insertDeploymentTemplate",
        "title": "Insert Deployment Template",
        "category": "DevEx"
      },
      {
        "command": "devex.addEndpoint",
        "title": "Add API Endpoint from OpenAPI",
        "category": "DevEx"
      },
      {
        "command": "devex.viewDashboard",
        "title": "View Productivity Dashboard",
        "category": "DevEx"
      },
      {
        "command": "devex.checkForUpdates",
        "title": "Check for Extension Updates",
        "category": "DevEx"
      },
      {
        "command": "devex.reviewCode",
        "title": "Review Code",
        "category": "DevEx"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceExtname == .md || resourceExtname == .txt || resourceExtname == .docx",
          "command": "devex.summarizeLLD",
          "group": "devex@1"
        },
        {
          "when": "resourceExtname == .md || resourceExtname == .txt || resourceExtname == .docx",
          "command": "devex.reviewLLD",
          "group": "devex@2"
        },
        {
          "when": "resourceExtname == .md || resourceExtname == .txt || resourceExtname == .docx",
          "command": "devex.generateOpenAPISpec",
          "group": "devex@3"
        },
        {
          "when": "resourceExtname == .yaml || resourceExtname == .yml || resourceExtname == .json",
          "command": "devex.parseOpenAPI",
          "group": "devex@4"
        }
      ],
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "devex.generateSpringBootProject",
          "group": "devex@1"
        },
        {
          "when": "explorerResourceIsFolder",
          "command": "devex.reviewCode",
          "group": "devex@2"
        },
        {
          "when": "resourceExtname == .md || resourceExtname == .txt || resourceExtname == .docx",
          "command": "devex.summarizeLLD",
          "group": "devex@2"
        },
        {
          "when": "resourceExtname == .md || resourceExtname == .txt || resourceExtname == .docx",
          "command": "devex.reviewLLD",
          "group": "devex@3"
        },
        {
          "when": "resourceExtname == .md || resourceExtname == .txt || resourceExtname == .docx",
          "command": "devex.generateOpenAPISpec",
          "group": "devex@4"
        },
        {
          "when": "resourceExtname == .yaml || resourceExtname == .yml || resourceExtname == .json",
          "command": "devex.parseOpenAPI",
          "group": "devex@5"
        }
      ]
    },
    "configuration": {
      "title": "DevEx AI Assistant",
      "properties": {
        "devex.defaultPackageName": {
          "type": "string",
          "default": "com.company",
          "description": "Default Java package name prefix"
        },
        "devex.buildTool": {
          "type": "string",
          "enum": [
            "maven",
            "gradle"
          ],
          "default": "maven",
          "description": "Default build tool for Spring Boot projects"
        },
        "devex.javaVersion": {
          "type": "string",
          "enum": [
            "17",
            "21"
          ],
          "default": "21",
          "description": "Java version for generated projects"
        },
        "devex.springBootVersion": {
          "type": "string",
          "default": "3.4.1",
          "description": "Spring Boot version to use"
        },
        "devex.telemetryEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable productivity tracking and metrics"
        },
        "devex.customTemplatesPath": {
          "type": "string",
          "default": "",
          "description": "Path to custom deployment templates (overrides built-in templates)"
        },
        "devex.updateCheckUrl": {
          "type": "string",
          "default": "",
          "description": "URL to check for extension updates (GitHub releases API)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "mammoth": "^1.11.0",
    "swagger-parser": "^10.0.3"
  }
}
